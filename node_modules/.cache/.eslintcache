minBody/Quran.js",["139"],[],"import { Card, Tab, ListGroup, Row, Col } from \"react-bootstrap\";\nimport { NavLink,useParams  } from \"react-router-dom\";\nimport axios from \"axios\";\nimport { useEffect, useState } from \"react\";\nimport Classess from \"../../../css/index.module.css\";\n\nexport default function Quran() {\n  const [quran, setQuran] = useState([]);\n  const [surah, setSurah] = useState([]);\n    let { al } = useParams();\n\n  //modal\n\n  useEffect(() => {\n  axios\n           .get(`${process.env.REACT_APP_URL}/quran/${al}`)\n           .then((allquran) => {\n             setQuran(allquran.data);\n           })\n           .catch((err) => console.log(err));\n \n  }, [al]);\n\n  //surah List\n  const listSurah = (id) => {\n  \n    axios\n      .get(`${process.env.REACT_APP_URL}/quran/surah/${id}`)\n      .then((search) => {\n        search.data.map((value) => {\n          setSurah(value);\n        });\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  };\n\n  //deleteItem\n  const deleteItem = (deleteId) => {\n    \n    axios\n      .delete(`${process.env.REACT_APP_URL}/quran/delete/${deleteId}`)\n      .then((value) => {\n        alert (value)\n      })\n      .catch((err) => {\n        console.log({message:err});\n      });\n  };\n\n  return (\n    <>\n      <Row>\n        <Col sm={3}>\n          <h3 className=\"text-center\">সুরা</h3>\n\n          <ListGroup>\n            {quran.map((value, key) => (\n              <ListGroup.Item\n                key={key}\n                className={`${Classess.unActive} text-center my-1 border border-warning`}\n              >\n                <p onClick={() => listSurah(value._id)}> {value.name}</p>\n              </ListGroup.Item>\n            ))}\n          </ListGroup>\n        </Col>\n        <Col sm={9}>\n          <Card border=\"secondary\" style={{ width: \"100%\" }}>\n            <Card.Header className=\" d-flex justify-content-around text-dark\">\n              <h2>{surah.name}</h2>\n              {quran.length > 0 ? (\n                <div className=\"d-flex text font-weight-bold\">\n                  <div\n                    className={`${Classess.unActive} mx-1 px-2 py-1 border border-warning`}\n                  >\n                    <NavLink\n                      className={`${Classess.unActive} mx-1 px-2 py-1  border border-warning`}\n                      to={`/edite/surah/${surah._id}`}\n                    >\n                      update\n                    </NavLink>\n                  </div>\n                  <div\n                    onClick={() => deleteItem(surah._id)}\n                    className={`${Classess.unActive} mx-1 px-2 py-1 border border-warning`}\n                  >\n                    delete\n                  </div>\n                </div>\n              ) : (\n                <div></div>\n              )}\n            </Card.Header>\n\n            {quran.length > 0 ? (\n              <Card.Body>\n                <Card.Text>\n                  {surah.verses}\n                  <span className=\"bg-warning text-secondary mx-2 p-1 font-weight-bold\">\n                    [{surah.name} - {surah.number}:{surah.versesNumber}]\n                  </span>\n\n                  <Tab.Container id=\"list-group-tabs-example\">\n                    <Row>\n                      <Col>\n                        <ListGroup>\n                          <ListGroup.Item action href=\"#link1\">\n                            সারসংক্ষেপ\"\n                          </ListGroup.Item>\n                          <ListGroup.Item action href=\"#link2\">\n                            আনুসঞ্জিক বিষয়\n                          </ListGroup.Item>\n                        </ListGroup>\n                      </Col>\n                    </Row>\n                    <Row>\n                      <Col>\n                        <Tab.Content>\n                          <Tab.Pane eventKey=\"#link1\">{surah.summary}</Tab.Pane>\n                          <Tab.Pane eventKey=\"#link2\">\n                            {surah.ancillary_issues}\n                          </Tab.Pane>\n                        </Tab.Content>\n                      </Col>\n                    </Row>\n                  </Tab.Container>\n                </Card.Text>\n              </Card.Body>\n            ) : (\n              <div className=\"bg-warning text-dark h3 text-center\">আপনার বইটি এখনো সংযুক্ত করা হয় নি </div>\n            )}\n          </Card>\n        </Col>\n      </Row>\n    </>\n  );\n}\n",["140","141"],"/home/ahsan/Documents/qurans/quran_client/src/components/admin/adminBody/Subject.js",["142"],[],"/home/ahsan/Documents/qurans/quran_client/src/components/mobile/CenterMobile.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/mobile/CenterMobileMain.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/mobile/CenterMobileSubject.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/header/Topbar.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/header/TopInput.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/Footer.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/center/CenterLaptop.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/center/Center.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/center/CenterSubject.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/header/TopSubject.js",[],[],"/home/ahsan/Documents/qurans/quran_client/src/components/admin/adminBody/Book.js",["143"],[],"/home/ahsan/Documents/qurans/quran_client/src/components/admin/edites/EditeSurah.js",["144","145"],[],"/home/ahsan/Documents/qurans/quran_client/src/components/admin/edites/EditeSubject.js",["146","147"],[],"/home/ahsan/Documents/qurans/quran_client/src/components/admin/edites/EditeBook.js",["148","149"],[],{"ruleId":"150","replacedBy":"151"},{"ruleId":"152","replacedBy":"153"},{"ruleId":"1